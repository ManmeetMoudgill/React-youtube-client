  "scripts": {
    "start": "env-cmd -f ./src/.env react-scripts start",
    "build": "env-cmd -f ./src/.env react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sass": "npm run sass-app && sass-comp",
    "sass-app": "node-sass -o src/app/css src/app/app.scss",
    "sass-app:watch": "node-sass -o src/app/css src/app/app.scss -w",
    "sass-comp": "node-sass -o src/components/css src/components/css/index.scss",
    "sass-comp:watch": "node-sass -o src/components/css src/components/css/index.scss -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/lodash.debounce": "^4.0.7",
    "@types/luxon": "^3.2.0",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26"
  }
